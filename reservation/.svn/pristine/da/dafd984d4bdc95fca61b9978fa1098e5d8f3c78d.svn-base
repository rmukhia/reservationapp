<div class="container">
    <h1>Develop</h1>
    <ul class="nav nav-tabs">
      <li class="active"><a>Rest APIs </a></li>
      <li><a href="{{AppConfig.rest_api_base}}" target="_blank">API Browser</a></li>
    </ul>
    <table class="table table-striped table-hover table_bg">
        <thead>
            <tr>
                <th>Model</th>
                <th>Url</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="(name, value) in models">
                <td>{{name}}</td>
                <td><a href="{{value}}" target="_blank">{{value}}</a></td>
            </tr>
        </tbody>
    </table>
</div>
